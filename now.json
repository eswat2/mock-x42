{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/id",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/vins",
      "dest": "/server.js",
      "methods": ["GET"]
    }
  ]
}
