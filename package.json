{
  "name": "mock-x42",
  "version": "0.0.124",
  "private": true,
  "main": "server.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "chance": "1.1.12",
    "express": "5.1.0",
    "fs": "0.0.2",
    "node-cache": "5.1.2",
    "vin-generator": "1.0.1"
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "test": "mocha --recursive",
